<h1>Create Heroine:</h1>
  <% if @heroine.errors %>
    <% @heroine.errors.full_messages.each do |message| %>
      <%= message %> 
    <%end %>
  <%end %>

<%= form_for @heroine do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br>

    <%= f.label :super_name %>
    <%= f.text_field :super_name %> <br>

    <%= f.collection_select :power_id, Power.all, :id, :name %> <br>

    <%= f.submit %>
    <%end%>
