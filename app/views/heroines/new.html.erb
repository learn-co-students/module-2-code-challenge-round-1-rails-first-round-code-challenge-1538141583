<h2> Submit your name and power for further consideration by the Justice League </h2>
<%= form_for(@heroine) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %><br>
  <%= f.label :power_id %>
  <%= f.collection_select :power_id, Power.all, :name, :name%><br>
  <%= f.submit 'Submit' %>
<%end%>
