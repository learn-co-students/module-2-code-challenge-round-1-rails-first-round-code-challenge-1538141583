<% if @heroine.errors %>
  <% @heroine.errors.full_messages.each do |message|%>
    <li><%=message%></li>
  <%end%>
<%end%>



<%= form_for(@heroine) do |f| %>
<%= f.label :name   %>
<%= f.text_area :name %> <br>
<%= f.label :super_name %>
<%= f.text_area :super_name %><br>
<%= f.collection_select(:power_id, Power.all, :id, :name) %> <br>
<%= f.submit %>
<%end%>
