<h2>All Heroines</h2>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%=link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>


<%= form_tag(heroines_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search power" %>
<%= submit_tag "Search" %>
<% end %>

<%if params[:search] %>
<%= @heroines_with_power.each do |heroine| %>
<%= heroine.name %>
<% end %>
<% end %>
